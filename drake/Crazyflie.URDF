<?xml version="1.0" ?>
<robot xmlns="http://drake.mit.edu"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 name="crazyflie">
  
  <link name="base_link">
    <inertial>
      <mass value="0.0227"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="8.5020E-12" ixy="0.0" ixz="0.0" iyy="8.5020E-12" iyz="0.0" izz="5.5805E-12"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="crazyflie_base.obj" scale=".05"/>
      </geometry>
    </visual>
  </link>

  <force_element name="prop1">
    <propellor lower_limit="0" upper_limit="5.625E9" scale_factor_thrust="1" scale_factor_moment="-1" visual_geometry="true">
      <parent link="base_link"/>
      <origin xyz="0.043 0 0"/>
      <axis xyz="0 0 1"/>
    </propellor>
  </force_element>
  <force_element name="prop2">
    <propellor lower_limit="0" upper_limit="5.625E9" scale_factor_thrust="1" scale_factor_moment="1" visual_geometry="true">
      <parent link="base_link"/>
      <origin xyz="0 -0.043 0 "/>
      <axis xyz="0 0 1"/>
    </propellor>
  </force_element>
  <force_element name="prop3">
    <propellor lower_limit="0" upper_limit="5.625E9" scale_factor_thrust="1" scale_factor_moment="-1" visual_geometry="true">
      <parent link="base_link"/>
      <origin xyz="-0.043 0 0"/>
      <axis xyz="0 0 1"/>
    </propellor>
  </force_element>
  <force_element name="prop4">
    <propellor lower_limit="0" upper_limit="5.625E9" scale_factor_thrust="1" scale_factor_moment="1" visual_geometry="true">
      <parent link="base_link"/>
      <origin xyz="0 0.043 0"/>
      <axis xyz="0 0 1"/>
    </propellor>
  </force_element>
  
</robot>