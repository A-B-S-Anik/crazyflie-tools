package crazyflie_t;

struct crazyflie_pos_t
{
  int64_t timestamp;
  double x;
  double y;
  double z;
  double roll;
  double pitch;
  double yaw;
}

struct crazyflie_input_t
{
  int64_t timestamp;
  double input[4];
  double offset;
  string type;
}

struct crazyflie_state_estimate_t
{
  int64_t timestamp;
  double xhat[12];
}

struct crazyflie_imu_t
{
  int64_t timestamp;
  double roll;
  double pitch;
  double yaw;
  double rolld;
  double pitchd;
  double yawd;
}
