package crazyflie_t;

struct crazyflie_pos_t
{
  int64_t timestamp;
  double x;
  double y;
  double z;
  double roll;
  double pitch;
  double yaw;
}

struct crazyflie_thrust_t
{
  int64_t timestamp;
  int16_t thrust1;
  int16_t thrust2;
  int16_t thrust3;
  int16_t thrust4;
}

struct crazyflie_imu_t
{
  int64_t timestamp;
  double roll;
  double pitch;
  double yaw;
}

struct crazyflie_state_estimate_t
{
  int64_t timestamp;
  double xhat[12];
}
